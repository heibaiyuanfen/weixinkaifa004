.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 50rpx 0;
}
.btn-container {
  display: flex;
  gap: 20rpx;
}
button {
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 8rpx;
  background-color: #1aad19;
  color: #fff;
}
.tips {
  margin-top: 30rpx;
  color: #666;
  font-size: 24rpx;
}
